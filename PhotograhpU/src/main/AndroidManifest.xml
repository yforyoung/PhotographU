<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.y.photographu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".view.activity.PhotographApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".view.activity.IntroduceActivity" />
        <activity android:name=".view.activity.TypeShowActivity" />
        <activity
            android:name=".view.activity.LoadActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.RegisterActivity" />
        <activity android:name=".view.activity.FindPasswordActivity" />
        <activity android:name=".view.activity.SchoolChooseActivity" />
        <activity android:name=".view.activity.SettingActivity" />
        <activity android:name=".view.activity.UserInfoSetActivity" />
        <activity android:name=".view.activity.ApplyPhotographerActivity"></activity>
    </application>

</manifest>